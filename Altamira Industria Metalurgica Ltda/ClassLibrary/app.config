<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="SA" connectionString="Data Source=sqlserver;Initial Catalog=SA;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
    <requiredRuntime version="v4.0"/>
  </startup>
</configuration>
